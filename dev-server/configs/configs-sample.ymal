users:
  - name: "coder"
    uid: "1000"
    gid: "100"
    password: "password"
    directories:
      - name: "home"
        path: "/home/coder"
        mode: "755"
      - name: "resources"
        path: "/resources"
        mode: "755"
      - name: "workspace"
        path: "/workspace"
        mode: "755"
      - name: "data"
        path: "/data"
        mode: "755"
      - name: "apps"
        path: "/apps"
        mode: "755"
      - name: "app"
        path: "/apps/app"
        mode: "755"
    backup_paths:
      - "/home/coder/.config"
      - "/home/coder/.ssh"
      - "/home/coder/.zshrc"
      - "/home/coder/.bashrc"
      - "/home/coder/.profile"
      - "/home/coder/.condarc"
      - "/home/coder/.oh-my-zsh"
      - "/home/coder/.gitconfig"
      - "/home/coder/filebrowser.db"
      - "/home/coder/.local"
      - "/home/coder/.conda"
      - "/home/coder/.vscode"
      - "/home/coder/.jupyte"
    zsh:
        set_theme: "spaceship"
        theme:
          - "https://github.com/romkatv/powerlevel10k"
          - "https://github.com/denysdovhan/spaceship-prompt"
          - "https://github.com/sobolevn/sobole-zsh-theme"
        plugins:
          - "git"
          - "k"
          - "extract"
          - "cp"
          - "yarn"
          - "npm"
          - "supervisor"
          - "rsync"
          - "command-not-found"
          - "autojump"
          - "colored-man-pages"
          - "git-flow"
          - "git-extras"
          - "python"
          - "zsh-autosuggestions"
          - "history-substring-search"
          - "zsh-completions"
          - "ssh-agent"
          - "https://github.com/zsh-users/zsh-autosuggestions"
          - "https://github.com/zsh-users/zsh-completions"
          - "https://github.com/zsh-users/zsh-syntax-highlighting"
          - "https://github.com/zsh-users/zsh-history-substring-search"
          - "https://github.com/supercrabtree/k"
    ssh:
        authorized_keys:
          - "ssh-rsa AAAAB3N_test_key"
        key_names:
          - "test_key"
        configs:
          - name: "test.git.proneer.co"
            hostname: "git.proneer.co"
            port: "22"
            user: "git"
            pub_key_auth: "yes"
            id_only: "yes"
            id_file_path: "~/.ssh/id_ed25519"
    filebrowser:
        port: "8055"
        base_url: "/data"
        root_dir: "/workspace"
    vscode:
        bind_addr: "0.0.0.0:8300"
        base_url: "/code"
        extensions: 
          - "ms-python.python"
          - "almenon.arepl"
          - "batisteo.vscode-django"
          - "bierner.color-info"
          - "bierner.markdown-footnotes"
          - "bierner.markdown-preview-github-styles"
          - "CoenraadS.bracket-pair-colorizer-2"
          - "DavidAnson.vscode-markdownlint"
          - "donjayamanne.githistory"
          - "eamodio.gitlens"
          - "hbenl.vscode-test-explorer"
          - "kamikillerto.vscode-colorize"
          - "kisstkondoros.vscode-gutter-preview"
          - "littlefoxteam.vscode-python-test-adapter"
          - "magicstack.MagicPython"
          - "ms-azuretools.vscode-docker"
          - "ms-toolsai.jupyter"
          - "naumovs.color-highlight"
          - "shd101wyy.markdown-preview-enhanced"
          - "streetsidesoftware.code-spell-checker"
          - "wholroyd.jinja"
          - "yzhang.markdown-all-in-one"
    app:
        bind_addr: "0.0.0.0:8080"
        base_url: "/"
        root_dir: "/workspace/admin"
        user: "admin"
        password: "adminpass1234"
